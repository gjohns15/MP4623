<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.uark.jfrorieemail.scurry">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".LoginScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainScreen"/>
        <activity android:name=".PostingForm"/>
        <activity android:name=".ProfilePage"/>
        <activity android:name=".ProfileSettings" />
        <activity android:name=".SearchResults"/>
        <activity android:name=".CreateAccount"/>
        <activity android:name=".Messages" />


    </application>

</manifest>
